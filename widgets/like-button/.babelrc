{
  "plugins": [
    [
      "babel-plugin-transform-goober",
      {
        "name": "_styled"
      }
    ],
    [
      "@babel/plugin-transform-react-jsx",
      {
        "pragma": "h",
        "pragmaFrag": "Fragment"
      }
    ],
    "babel-plugin-macros"
  ],

  "env": {
    "test": {
      "presets": [
        ["preact-cli/babel", { "modules": "commonjs" }]
      ]
    }
  }
}
